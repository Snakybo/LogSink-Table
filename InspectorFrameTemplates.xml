<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\FrameXML\UI.xsd">
	<Button name="LogInspectorRowTemplate" virtual="true">
		<Layers>
			<Layer level="ARTWORK">
				<FontString parentKey="Key" inherits="ChatFontNormal" justifyH="LEFT">
					<Size x="0" y="0"/>
					<Anchors>
						<Anchor point="TOPLEFT" x="4" y="0"/>
						<Anchor point="BOTTOMLEFT" x="4" y="0"/>
					</Anchors>
				</FontString>

				<FontString parentKey="Value" inherits="ChatFontNormal" justifyH="LEFT">
					<Size x="0" y="0"/>
					<Anchors>
						<Anchor point="BOTTOMRIGHT" x="-4" y="0"/>
						<Anchor point="TOPLEFT" relativeKey="$parent.Key" relativePoint="TOPRIGHT" x="10" y="0"/>
					</Anchors>
				</FontString>
			</Layer>
		</Layers>
	</Button>
</Ui>
